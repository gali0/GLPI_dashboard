{
  "description": "GLPI dashboard",
  "manifest_version": 2,
  "name": "GLPI dashboard",
  "version": "1.0",

  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/main.html",
    "default_icon": {
      "16": "icons/LogoGlpi-16.png",
      "32": "icons/LogoGlpi-32.png"
    }
  },

  "permissions": ["storage"],

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true,
    "chrome_style": true,
    "open_in_tab": false
  }

}